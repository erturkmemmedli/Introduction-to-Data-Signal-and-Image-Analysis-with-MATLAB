Quiz 1 - Reverse Audio

load ReverseAudio.mat
% [y_orig,Fs,format] = wavread('---')
reverse = y_rev(:,1);
original = flipud(reverse)
subplot(2,1,1)
plot (reverse)
title('Reverse')
subplot(2,1,2)
plot (original)
title('Original')
% sound(reverse, Fs)
sound(original,Fs)

# matlab

Quiz 2 - 

function MagnitudeSpectrumPlot(yfft,f,col)
    if nargin<3
        col = 'b'
    end
    plot(f,abs(yfft),col);
    xlabel('Frequency (Hz)')
    ylabel('|Y(f)|')
end
load crickets.mat
soundsc(crickets,Fs)
[yfft, f] = myfft(crickets,Fs);
MagnitudeSpectrumPlot(yfft,f,'*')

